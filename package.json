{
	"name": "new-fractal-folder",
	"displayName": "New Fractal Folder",
	"description": "Create a twig, scss, and config.js file with the same name of a new folder",
	"version": "0.0.1",
	"publisher": "baerkins",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.createFractalFolder"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.createFractalFolder",
				"title": "Create New Fractal Folder"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.createFractalFolder",
					"group": "9_cutcopypaste"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
